<li class="dropdown-submenu">
<a tabindex="-1" href="#"><%= timetable.name %><%= " (active)" if timetable.active? %></a>
  <ul class="dropdown-menu">
    <li>
      <%= link_to calendar_event_sources_path(timetable.calendar) do %>
        Event Sources
      <% end %>
    </li>
    <% unless timetable.active? %>
      <li>
        <%= link_to timetable_make_active_path(timetable), class: "activate-timetable", remote: true, method: :put do %>
          Make this my active timetable!
        <% end %>
      </li>
    <% end %>
  </ul>
</li>

