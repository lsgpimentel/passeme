<div class="row-fluid">
  <div class="span12">
    <h4 class="show-results-timetable"><%= t('.results_for') %>: &nbsp;&nbsp;&nbsp;</h4>

    <select id="timetable-select" style="width: 45%;" data-url-callback="<%= change_timetable_report_path %>">
      <% current_user.timetables.each do |t| %>
        <option value="<%= t.id %>" <%= 'selected' if @timetable == t %>><%= t.name %></option>
      <% end %>
    </select>
  </div>
</div>
<hr>

<div class="row-fluid">
  <div class="span12">
    <%= render 'study_hours_evolution' %>
  </div>
</div>
<div class="row-fluid">
  <div class="span6">
    <%= render 'expected_and_accomplished' %>
  </div>
  <div class="span6">
    <%= render 'follow_up_not_studied_reason' %>
  </div>
</div>

