<% content_for :stylesheets do %>
  <%= stylesheet_link_tag "authenticated_and_devise", media: "all", "data-turbolinks-track" => true %>
<% end %>

<% content_for :content do %>

<body class="page-header-fixed">


	<!-- BEGIN HEADER -->   
  <%= render 'authenticated/header' %>
  <!-- END HEADER -->


	<!-- BEGIN CONTAINER -->   
  <div class="page-container row-fluid">

		<!-- BEGIN SIDEBAR -->
    <%= render 'authenticated/sidebar' %>
    <!-- END SIDEBAR -->

		<!-- BEGIN PAGE -->
		<div class="page-content">
      <!-- BEGIN PAGE CONTAINER-->
      <div class="container-fluid">
        <!-- BEGIN PAGE HEADER-->
        <%= render 'authenticated/content_header' %>
        <!-- END PAGE HEADER-->
        <!-- BEGIN PAGE CONTENT-->
        <div class="row-fluid">
          <div class="span12">
            <%= content_for?(:page_content) ? yield(:page_content) : yield %>
          </div>
        </div>
        <!-- END PAGE CONTENT-->
      </div>
      <!-- END PAGE CONTAINER--> 
    </div>
    <!-- END PAGE -->    

  </div>
  <!-- END CONTAINER -->


  <!-- BEGIN FOOTER -->
  <%= render 'authenticated/footer' %>
  <!-- END FOOTER -->


  <!-- BEGIN CORE PLUGINS -->
  <%= javascript_include_tag "authenticated_and_devise", "data-turbolinks-track" => true %>

  <!--[if lt IE 9]>
  <%= javascript_include_tag "authenticated_and_devise-ie", "data-turbolinks-track" => true %>
  <![endif]-->   

  <!-- END JAVASCRIPTS -->
  </body>

<% end %>
  <%= render template: "layouts/application" %>
